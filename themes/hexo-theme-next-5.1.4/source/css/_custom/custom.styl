// Custom styles.

body {
    background:linear-gradient(rgba(0, 0, 0, 0.1), rgba(0, 0, 0, 0.1)), url("https://images2.alphacoders.com/401/40102.jpg");
    background-repeat: no-repeat;
    background-attachment:fixed;
    background-position:50% 50%;
}

.header {
    background: linear-gradient(rgba(0, 0, 0, 0.8), rgba(0, 0, 0, 0.8)), url("https://images.alphacoders.com/242/242993.jpg") no-repeat;
    background-size: cover;
    background-color: black;
    background-position: center;
    width: 100%;
    display: table;
}

.main-inner { 
    margin-top: 60px;
    padding: 60px 60px 60px 60px;
    background: #fff;
    min-height: 500px;
}


